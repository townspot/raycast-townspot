{
  "name": "raycast-townspot",
  "title": "TownSpot",
  "description": "Raycast command to ask TownSpot what's on.",
  "version": "0.0.1",
  "private": true,
  "categories": [
    "Fun"
  ],
  "main": "dist/index.js",
  "author": "mikegyi",
  "license": "MIT",
  "icon": "icon.png",
  "scripts": {
    "dev": "ray dev",
    "build": "ray build",
    "lint": "ray lint"
  },
  "dependencies": {
    "@raycast/api": "^1.89.2"
  },
  "devDependencies": {
    "@types/node": "^20.16.5",
    "@typescript-eslint/parser": "^8.55.0",
    "typescript": "^5.8.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "commands": [
    {
      "name": "ask",
      "title": "TownSpot",
      "icon": "icon.png",
      "description": "Ask what's on in your town with conversational search.",
      "mode": "view",
      "path": "ask",
      "keywords": [
        "events",
        "townspot",
        "weekend",
        "music"
      ],
      "preferences": [
        {
          "name": "locale",
          "title": "Locale",
          "description": "API locale (en-GB, es-ES, ca-ES)",
          "type": "textfield",
          "required": true,
          "default": "en-GB"
        }
      ]
    },
    {
      "name": "ask-townspot-ai",
      "title": "Ask TownSpot AI",
      "icon": "icon.png",
      "description": "Ask with Raycast AI, grounded by verified TownSpot listings.",
      "mode": "view",
      "path": "ask-townspot-ai",
      "keywords": [
        "ai",
        "townspot",
        "events",
        "chat"
      ],
      "arguments": [
        {
          "name": "prompt",
          "placeholder": "Ask TownSpot AI",
          "type": "text",
          "required": false
        }
      ],
      "preferences": [
        {
          "name": "locale",
          "title": "Locale",
          "description": "API locale (en-GB, es-ES, ca-ES)",
          "type": "textfield",
          "required": true,
          "default": "en-GB"
        }
      ]
    }
  ]
}
